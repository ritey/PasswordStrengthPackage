{
    "name": "ritey/password-strength",
    "description": "This package provides a validator for ensuring strong passwords in Laravel 6 applications.",
    "keywords": [
        "laravel",
        "laravel 6",
        "validation",
        "password",
        "password strength"
    ],
    "authors": [
        {
            "name": "David Wright",
            "email": "dave@ritey.com"
        },
        {
            "name": "Oliver Schupp",
            "email": "oliver.schupp@yahoo.de"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.0|6.*",
        "illuminate/translation": "^5.1|6.*"
    },
    "require-dev": {
        "illuminate/validation": "~5.0",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "Schuppo\\PasswordStrength": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schuppo\\PasswordStrength\\PasswordStrengthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "license": "MIT"
}
